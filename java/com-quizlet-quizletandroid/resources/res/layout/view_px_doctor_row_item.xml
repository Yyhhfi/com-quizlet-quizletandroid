<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/row_item_circle_view"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="38dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textSize="12sp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:id="@+id/row_item_number_text_view"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginTop="2dp"
        android:text=""
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/row_item_circle_view"
        app:layout_constraintEnd_toEndOf="@+id/row_item_circle_view"
        app:layout_constraintStart_toStartOf="@+id/row_item_circle_view"
        app:layout_constraintTop_toTopOf="@+id/row_item_circle_view"/>
    <TextView
        android:textSize="19sp"
        android:textColor="@color/DOCTOR_DEFAULT_TEXT_COLOR"
        android:id="@+id/row_item_content_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="15dp"
        android:text=""
        android:layout_marginStart="15dp"
        android:layout_marginEnd="30dp"
        android:textFontWeight="300"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/row_item_circle_view"
        app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
