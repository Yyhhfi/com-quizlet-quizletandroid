<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraintLayout"
    android:descendantFocusability="blocksDescendants"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/summary_item_background_image_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/summary_item_content_view"
        android:background="#ffffff"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="5dp"
        android:layout_marginStart="47dp"
        android:layout_marginEnd="21dp"
        android:backgroundTint="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1"/>
    <TextView
        android:textSize="18sp"
        android:textColor="@color/DOCTOR_DEFAULT_TEXT_COLOR"
        android:id="@+id/summary_item_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="24dp"
        android:text="TextView"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="10dp"
        android:textFontWeight="500"
        app:layout_constraintBottom_toBottomOf="@+id/summary_item_content_view"
        app:layout_constraintEnd_toStartOf="@+id/summary_item_arrow_image_view"
        app:layout_constraintStart_toStartOf="@+id/summary_item_content_view"
        app:layout_constraintTop_toTopOf="@+id/summary_item_content_view"/>
    <TextView
        android:textSize="17sp"
        android:textColor="@color/DOCTOR_DEFAULT_TEXT_COLOR"
        android:id="@+id/summary_item_description_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginEnd="23dp"
        android:textFontWeight="300"
        app:layout_constraintEnd_toEndOf="@+id/summary_item_content_view"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/summary_item_title_text_view"/>
    <ImageView
        android:id="@+id/summary_item_arrow_image_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        app:layout_constraintBottom_toTopOf="@+id/summary_item_title_text_view"
        app:layout_constraintEnd_toEndOf="@+id/summary_item_content_view"
        app:layout_constraintTop_toBottomOf="@+id/summary_item_title_text_view"/>
    <View
        android:id="@+id/summary_item_top_line_view"
        android:background="@color/DOCTOR_TABLE_LINE_COLOR"
        android:layout_width="1dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/summary_item_mark_image_view"
        app:layout_constraintEnd_toStartOf="@+id/summary_item_mark_image_view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/summary_item_mark_image_view"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/summary_item_bottom_line_view"
        android:background="@color/DOCTOR_TABLE_LINE_COLOR"
        android:layout_width="1dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/summary_item_mark_image_view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/summary_item_mark_image_view"
        app:layout_constraintTop_toBottomOf="@+id/summary_item_mark_image_view"/>
    <ImageView
        android:id="@+id/summary_item_mark_image_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/summary_item_content_view"
        app:layout_constraintTop_toTopOf="@+id/summary_item_title_text_view"/>
    <TextView
        android:textSize="14sp"
        android:textStyle="bold"
        android:textColor="@color/DOCTOR_LINK_COLOR"
        android:id="@+id/summary_item_troubleshooting_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text=""
        app:layout_constraintStart_toStartOf="@+id/summary_item_description_text_view"
        app:layout_constraintTop_toBottomOf="@+id/summary_item_description_text_view"/>
    <TextView
        android:textSize="17sp"
        android:textColor="@color/DOCTOR_DEFAULT_TEXT_COLOR"
        android:id="@+id/summary_item_troubleshooting_value_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text=""
        android:textFontWeight="300"
        app:layout_constraintEnd_toEndOf="@+id/summary_item_description_text_view"
        app:layout_constraintStart_toStartOf="@+id/summary_item_troubleshooting_title_text_view"
        app:layout_constraintTop_toBottomOf="@+id/summary_item_troubleshooting_title_text_view"/>
</androidx.constraintlayout.widget.ConstraintLayout>
