<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/leftArrow"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:src="0x7f080376"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/hand_pointer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="?attr/AssemblyGray100"/>
    <ImageView
        android:id="@+id/hand_pointer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_hand_pointer"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="?attr/AssemblyGray100"/>
    <ImageView
        android:id="@+id/rightArrow"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:src="0x7f080375"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hand_pointer"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="?attr/AssemblyGray100"/>
    <com.quizlet.partskit.widgets.QTextView
        android:textColor="?attr/textColorInverse"
        android:id="@+id/tooltipOnboardingText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_medium"
        android:text="@string/search_set_preview_onboarding"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hand_pointer"
        style="@style/SubHeader.S1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
