    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/edit_set_language_gap"
        android:layout_width="match_parent"
        android:layout_height="@dimen/quizlet_edge_margin"/>
    <com.quizlet.partskit.widgets.QTextView
        android:textColor="?attr/textColorDisabled"
        android:id="@+id/edit_set_language_header"
