        android:paddingTop="@dimen/quizlet_edge_margin"
        android:paddingRight="@dimen/quizlet_edge_margin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/SubHeader.S5"/>
</LinearLayout>
