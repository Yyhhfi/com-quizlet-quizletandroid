<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.quizlet.partskit.widgets.QTabLayout
        android:id="@+id/tabLayout"
        android:fadingEdgeLength="@dimen/fading_edge_length"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:requiresFadingEdge="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tabContentStart="@dimen/ref_spacing_small"
        app:tabIndicatorColor="?attr/AssemblyButtonActiveState"
        app:tabIndicatorFullWidth="false"
        app:tabIndicatorHeight="2dp"
        app:tabMode="scrollable"
        app:tabSelectedTextColor="?attr/SysColorTextPrimary"
        app:tabTextAppearance="@style/SubHeader.S3"
        app:tabTextColor="?attr/SysColorTextSecondary"
        style="@style/Widget.Design.TabLayout"/>
</androidx.constraintlayout.widget.ConstraintLayout>
