<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/endOfMatch"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.quizlet.partskit.widgets.QTextView
        android:gravity="center"
        android:id="@+id/match_finish_text"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/you_finished_in"
        app:layout_constraintBottom_toTopOf="@+id/match_finish_seconds"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        style="@style/Body.B2"/>
    <com.quizlet.partskit.widgets.QTextView
        android:gravity="center"
        android:id="@+id/match_finish_seconds"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/match_personal_record"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/match_finish_text"
        style="@style/Header.H2"/>
    <com.quizlet.partskit.widgets.QTextView
        android:gravity="center"
        android:id="@+id/match_personal_record"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/you_finished_in"
        app:layout_constraintBottom_toTopOf="@+id/match_leaderboard_score"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/match_finish_seconds"
        style="@style/Body.B2"/>
    <androidx.recyclerview.widget.RecyclerView
        android:gravity="center"
        android:id="@+id/match_leaderboard_score"
        android:visibility="gone"
        android:fadingEdgeLength="@dimen/fading_edge_length"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:requiresFadingEdge="vertical"
        app:layout_constrainedHeight="true"
        app:layout_constraintBottom_toTopOf="@+id/match_results_button_group"
        app:layout_constraintTop_toBottomOf="@+id/match_personal_record"/>
    <com.quizlet.partskit.widgets.QTextView
        android:gravity="center"
        android:id="@+id/match_error"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:layout_constraintTop_toBottomOf="@+id/match_personal_record"
        style="@style/Body.B4"/>
    <com.quizlet.partskit.widgets.QProgressBar
        android:id="@+id/match_progress_bar"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:indeterminate="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/match_personal_record"
        style="?android:attr/progressBarStyle"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/match_results_button_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent">
        <com.quizlet.partskit.widgets.QButton
            android:id="@+id/match_play_again"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toTopOf="@+id/match_play_other_selected_terms_mode"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"/>
        <com.quizlet.partskit.widgets.QButton
            android:id="@+id/match_play_other_selected_terms_mode"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toTopOf="@+id/match_margin_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/match_play_again"/>
        <View
            android:id="@+id/match_margin_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/match_share_set_button"
            app:layout_constraintTop_toBottomOf="@+id/match_play_other_selected_terms_mode"
            app:layout_goneMarginBottom="16dp"/>
        <com.quizlet.quizletandroid.ui.common.views.ShareSetButton
            android:id="@+id/match_share_set_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/ref_spacing_small"
            android:layout_marginBottom="@dimen/ref_spacing_large"
            android:layout_marginStart="@dimen/ref_spacing_small"
            android:layout_marginEnd="@dimen/ref_spacing_small"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/match_play_again"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/match_play_again"
            app:layout_constraintTop_toBottomOf="@+id/match_margin_view"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
