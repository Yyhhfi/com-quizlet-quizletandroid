<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/mainContainer"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:orientation="vertical"
        android:id="@+id/snackbar_layout_wrapper"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/layout_collapsing_appbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.google.android.material.appbar.CollapsingToolbarLayout
                android:id="@+id/layout_collapsing_appbar_toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_scrollFlags="exitUntilCollapsed|scroll"
                app:titleEnabled="false">
                <LinearLayout
                    android:orientation="vertical"
                    android:background="?attr/SysColorBackground"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="?attr/actionBarSize">
                    <FrameLayout
                        android:id="@+id/setpage_study_preview_container"
                        android:background="?attr/SysColorBackground"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:animateLayoutChanges="true">
                        <FrameLayout
                            android:id="@+id/setpage_header_banner_ad"
                            android:background="?attr/SysColorBackground"
                            android:visibility="gone"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/quizlet_edge_margin"
                            android:layout_marginEnd="@dimen/quizlet_edge_margin"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.compose.ui.platform.ComposeView
                            android:id="@+id/setpage_header_subscription_ad"
                            android:background="?attr/SysColorBackground"
                            android:visibility="invisible"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/quizlet_edge_margin"
                            android:layout_marginEnd="@dimen/quizlet_edge_margin"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <FrameLayout
                        android:id="@+id/setpage_header_container"
                        android:background="?attr/SysColorBackground"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:id="@+id/set_progress_fragment_container"
                        android:background="?attr/SysColorBackground"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:id="@+id/set_selected_terms_mode_fragment_container"
                        android:background="?attr/SysColorBackground"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
                <androidx.appcompat.widget.Toolbar
                    android:visibility="invisible"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    app:layout_collapseMode="pin"/>
                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/toolbar"
                    android:background="?attr/SysColorBackground"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    app:layout_collapseMode="pin"/>
            </com.google.android.material.appbar.CollapsingToolbarLayout>
        </com.google.android.material.appbar.AppBarLayout>
        <RelativeLayout
            android:id="@+id/setpage_loading_spinner_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="200dp">
            <ProgressBar
                android:id="@+id/setpage_loading_spinner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <FrameLayout
            android:id="@+id/term_list_fragment_container"
            android:background="?attr/SysColorBackground"
            android:paddingBottom="@dimen/study_this_set_container_height"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"/>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
    <com.quizlet.uicommon.ui.common.views.SimpleGradientView
        android:id="@+id/bottom_fading_edge"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="24dp"
        app:endColor="?attr/SysColorBackground"
        app:gradientOrientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/setpage_study_set_button_container"/>
    <FrameLayout
        android:id="@+id/setpage_footer_banner_ad"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ref_spacing_xsmall"
        android:layout_marginStart="@dimen/ref_spacing_xsmall"
        android:layout_marginEnd="@dimen/ref_spacing_xsmall"
        app:layout_constraintBottom_toTopOf="@+id/setpage_study_set_button_container"/>
    <FrameLayout
        android:id="@+id/setpage_study_set_button_container"
        android:background="?attr/SysColorBackground"
        android:paddingTop="@dimen/study_this_set_vertical_margin"
        android:visibility="gone"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="@dimen/study_this_set_container_height"
        android:paddingStart="@dimen/study_this_set_horizontal_margin"
        android:paddingEnd="@dimen/study_this_set_horizontal_margin"
        app:layout_constraintBottom_toBottomOf="parent">
        <com.quizlet.partskit.widgets.QButton
            android:id="@+id/setpage_study_set_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/setpage_study_this_set"
            style="@style/QuizletSmallButton"/>
    </FrameLayout>
    <com.quizlet.features.achievements.achievement.AchievementEarnedView
        android:id="@+id/achievementToast"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ref_spacing_small"
        android:layout_marginHorizontal="@dimen/ref_spacing_small"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <View
        android:id="@+id/snackbar_anchor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:translationZ="1dp"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
