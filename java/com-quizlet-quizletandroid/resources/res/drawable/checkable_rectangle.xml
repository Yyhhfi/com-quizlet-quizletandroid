<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:drawable="@drawable/test_rectangle_correct"
        android:state_activated="true"/>
    <item
        android:state_enabled="false"
        android:state_checked="true"
        android:drawable="@drawable/test_rectangle_incorrect"
        android:state_activated="false"/>
    <item
        android:state_enabled="true"
        android:state_checked="true"
        android:drawable="@drawable/test_rectangle_checked"/>
    <item
        android:state_enabled="true"
        android:state_checked="false"
        android:drawable="@drawable/test_rectangle_disabled"
        android:state_activated="true"/>
    <item
        android:state_enabled="false"
        android:state_checked="false"
        android:drawable="@drawable/test_rectangle_disabled"/>
    <item
        android:state_checked="false"
        android:drawable="@drawable/test_rectangle_unchecked"/>
</selector>
