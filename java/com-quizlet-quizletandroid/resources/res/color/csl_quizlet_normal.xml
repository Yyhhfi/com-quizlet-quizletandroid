<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:state_selected="true"
        android:color="@color/assembly_sunset400"/>
    <item
        android:state_focused="true"
        android:color="?attr/AssemblyMint500"
        app:state_success="true"
        app:state_validated="true"/>
    <item
        android:color="?attr/AssemblyMint500"
        app:state_edittext_focus="true"
        app:state_success="true"
        app:state_validated="true"/>
    <item
        android:color="@color/assembly_error_red_dark"
        app:state_success="false"
        app:state_validated="true"/>
    <item
        android:state_focused="true"
        android:color="@color/assembly_sunset400"/>
    <item
        android:state_enabled="false"
        android:color="?attr/AssemblyIconColorDisabled"/>
    <item android:color="?attr/AssemblyIconColor"/>
</selector>
